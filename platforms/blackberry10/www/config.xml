<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kmedia.fucs" version="1.2.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:rim="http://www.blackberry.com/ns/widgets">
    <feature name="Device" value="Device" />
    <feature name="Notification" value="Notification" />
    <feature name="Geolocation" value="Geolocation" />
    <rim:permissions>
        <rim:permit>read_device_identifying_information</rim:permit>
        <rim:permit>read_geolocation</rim:permit>
    </rim:permissions>
    <name>FUCS</name>
    <description>√Åplication develop by mposadar</description>
    <author email="mposadar@gmail.com" href="http://algoritmo.solutions/mposadar/">Miguel Angel Posada</author>
    <content src="index.html" />
    <access subdomains="true" uri="http://fucs.kmedia.co" />
    <access subdomains="true" uri="https://google.com" />
    <access subdomains="true" uri="https://fonts.googleapis.com" />
    <access subdomains="true" uri="https://fonts.gstatic.com" />
    <access origin="*" />
    <feature id="blackberry.app">
        <param name="websecurity" value="disable" />
    </feature>
    <preference name="Orientation" value="portrait" />
</widget>
